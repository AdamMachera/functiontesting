{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information",
      "Host.Results": "Warning",
      "Function": "Warning",
      "Host.Aggregator": "Trace",
      "Host.Triggers.DurableTask": "Information"
    }
  },
  "extensions": {
    "durableTask": {
        "logReplayEvents": false
    }
  }
}
